		<if test="ts == 'ts'">
				 and TS_ID = '2'
		</if>
		<if test="imexport != 'all'">
		 	   	 and (substr(cur_stat, 1, 1) = #{imexport} or (substr(cur_stat, 1, 1) = 'M' and pod = 'ZZOPI'))
		</if>	 
		<if test="fm != 'all'">
				 and fm_cd = #{fm}
		</if>
		<if test="type != 'all'">
				 and F_TYPE_MAP(ty_cd) = #{type}
		</if>
		<if test="size != 'all'">
				 and F_SIZE_MAP(sz_cd, 1) = #{size}
		</if>
	</select>
</mapper>

